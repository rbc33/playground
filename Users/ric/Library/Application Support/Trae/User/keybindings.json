[
    {
        "key": "[Backslash] [Backslash]",
        "command": "notebook.cell.quitEdit",
        "when": "inputFocus && notebookEditorFocused && vim.active && !editorHasSelection && !editorHoverVisible && vim.mode == 'Normal'"
    },
    {
        "key": "ctrl+shift+s",
        "command": "workbench.action.toggleLockedScrolling"
    },
    {
        "command": "vscode-neovim.compositeEscape1",
        "key": "รง",
        "when": "neovim.mode == insert && editorTextFocus",
        "args": "รง"
    },
    {
        "command": "vscode-neovim.compositeEscape2",
        "key": "รง",
        "when": "neovim.mode == insert && editorTextFocus && neovim.recording",
        "args": "รง"
    }
]